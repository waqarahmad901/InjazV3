@using TranningWebApp.Resource
@model TranningWebApp.Repository.DataAccess.session
@using TmsWebApp.Common;
@using TranningWebApp.Common
@{

    var cu = Session["user"] as ContextUser;
}
<script src="~/Scripts/jquery.timepicker.min.js"></script>
<link href="~/Content/jquery.timepicker.min.css" rel="stylesheet" />
<script type="text/javascript">
    $(document).ready(function () {
        var role = "@cu.EnumRole";
        bindTimePicker();
        if (role.toLocaleLowerCase() == "coordinator") {
            if ("@Model.Status" == "Pending") {
                $('input')
                    .not(".actualdate")
                    .not("#NumberOfActualStudents")
                    .not("#@Html.IdFor(x=>x.Id)")
                    .not("#@Html.IdFor(x=>x.IsActive)")
                    .attr("disabled", "disabled");
                $(".check").parent().hide();
                bindDatedLocal();
            }
            if ("@Model.Status" == "Approved") {
                $('input').not("#@Html.IdFor(x=>x.Id)").not("#@Html.IdFor(x=>x.IsActive)").attr("disabled", "disabled");
                if ("@Model.SendKitByMailCourier" == "True") {
                    $("#@Html.IdFor(x=>x.ConfirmKitReceivedBySchool)").removeAttr("disabled");
                }

                $("#@Html.IdFor(x=>x.SendKitByMailCourier)").attr("disabled", "disabled");
                $("#@Html.IdFor(x=>x.ApprovedByAdmin)").attr("disabled", "disabled");
                $("#@Html.IdFor(x=>x.SessionOccured)").parent().hide();
                $("#@Html.IdFor(x=>x.MarkedCompletedByVolunteer)").parent().hide();
            }
            if ("@Model.Status" == "Occured") {
                $('input').not("#@Html.IdFor(x=>x.Id)").not("#@Html.IdFor(x=>x.EvaluationImageLink)").not("#@Html.IdFor(x=>x.SessionImageLink)").attr("disabled", "disabled");
                $('select').attr("disabled", "disabled");
            }

            $('select').attr("disabled", "disabled");
        }
        if (role.toLocaleLowerCase() == "superadmin") {
            if ("@Model.Status" == "DateChanges" || "@Model.Status" == "StudentChanges") {
                $('input').not("#@Html.IdFor(x=>x.Id)").not("#@Html.IdFor(x=>x.IsActive)").not("#@Html.IdFor(x=>x.SendKitByMailCourier)").attr("disabled", "disabled");
                $('select').attr("disabled", "disabled");
                $("#@Html.IdFor(x=>x.ConfirmKitReceivedBySchool)").parent().hide();
                $("#@Html.IdFor(x=>x.ApprovedByAdmin)").parent().hide();
                $("#@Html.IdFor(x=>x.SessionOccured)").parent().hide();
                $("#@Html.IdFor(x=>x.MarkedCompletedByVolunteer)").parent().hide();
            }
            if ("@Model.Status" == "Pending") {
                $(".actualdate").parent().hide();
                $("#NumberOfActualStudents").parent().hide();
                $(".check").parent().hide();
            }
            if ("@Model.Status" == "Approved") {
                $('input').not("#@Html.IdFor(x => x.Id)").not("#@Html.IdFor(x=>x.IsActive)").attr("disabled", "disabled");
                $('select').attr("disabled", "disabled");
                $("#@Html.IdFor(x => x.SessionOccured)").parent().hide();
                $("#@Html.IdFor(x => x.MarkedCompletedByVolunteer)").parent().hide();
            }
            if ("@Model.Status" == "Occured") {
                $('input').not("#@Html.IdFor(x=>x.Id)").not("#@Html.IdFor(x=>x.IsActive)").attr("disabled", "disabled");
                $('select').attr("disabled", "disabled");
            }
        }
        if (role.toLocaleLowerCase() == "volunteer") {
            $('input').not("#@Html.IdFor(x=>x.Id)").not("#@Html.IdFor(x=>x.IsActive)").not("#@Html.IdFor(x => x.RequestThisSession)").attr("disabled", "disabled");
            $('select').attr("disabled", "disabled");
            $("#@Html.IdFor(x=>x.ConfirmKitReceivedBySchool)").parent().hide();
            $("#@Html.IdFor(x=>x.ApprovedByAdmin)").parent().hide();
            $("#@Html.IdFor(x=>x.SessionOccured)").parent().hide();
            $("#@Html.IdFor(x => x.SendKitByMailCourier)").parent().hide();
            $("#@Html.IdFor(x => x.MarkedCompletedByVolunteer)").parent().hide();
            if ("@Model.Status" == "Occured") {
                $("#@Html.IdFor(x => x.VolunteerMarkedStudentAttendenceInSession)").removeAttr("disabled");
            }
        }
        if (role.toLocaleLowerCase() == "participant") {
            $('input').not("#@Html.IdFor(x=>x.Id)").not("#@Html.IdFor(x=>x.IsActive)").attr("disabled", "disabled");
            $('select').attr("disabled", "disabled");
            $(".check").parent().hide();
        }

    });
    function changeDate() {
        $(".actualdate").removeAttr("disabled");
        bindDatedLocal();
    }

    function bindDatedLocal() {
        $(".date-islamic-1").calendarsPicker({
            calendar: $.calendars.instance('islamic'),
            dateFormat: 'yyyy/mm/dd',
            onSelect: function (dates) {

                var dateselect = dates[0]._day + "-" + dates[0]._month + "-" + dates[0]._year;
                var control = $(this);
                $.get("/Home/ConvertDateCalendar?dateConv=" + dateselect + "&calendar=Gregorian&dateLangCulture=en-us", function (res) {
                    control.parent().parent().find(".date-1").val(res);
                    renderTimeControl("actualstart", "actualend");
                });
            }
        });

        $(".date-1").calendarsPicker({
            calendar: $.calendars.instance('gregorian'),
            dateFormat: 'dd/mm/yyyy',
            onSelect: function (dates) {
                renderTimeControl("actualstart", "actualend");
                var dateselect = dates[0]._day + "-" + dates[0]._month + "-" + dates[0]._year;
                var control = $(this);
                $.get("/Home/ConvertDateCalendar?dateConv=" + dateselect + "&calendar=Hijri&dateLangCulture=en-us", function (res) {
                    control.parent().parent().find(".date-islamic-1").val(res);

                });

            }
        });
    }

    function getFilterSchoolAndVolunteer()
    {
        var city = $("#City").val();
        var gender = $("#Gender").val();
        var tg = $("#TargetGroup").val();

        $.get("/Session/GetSchoolsVolunteerByFilter?city=" + city + "&tg=" + tg + "&gender=" + gender, function (res) {
            debugger;
            var sel = "@General.Select";
            $("#VolunteerId option").remove();
            $("#VolunteerId").append("<option value=''>" + sel + "</option>");
            $.each(res.volunteers, function (index, item) {
                $("#VolunteerId").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
            });

            $("#SchoolID option").remove();
            $("#SchoolID").append("<option value=''>" +sel +"</option>");
            $.each(res.schools, function (index, item) {
                $("#SchoolID").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
            });

        })
    }


</script>

<main class="main">

