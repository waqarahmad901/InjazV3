                                        @Html.CheckBoxFor(x => x.ConfirmKitReceivedBySchool, new { @style = "margin-top:5px", @class = "check" })
                                        <label style="margin-left:10px">@SessionX.ConfirmKitReceivedBySchool</label>
                                    </div>
